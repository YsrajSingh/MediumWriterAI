generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model test_table {
  id         Int       @id @default(autoincrement())
  name       String    @db.VarChar(50)
  age        Int?
  email      String?   @db.VarChar(100)
  created_at DateTime? @default(now()) @db.Timestamp(6)
}
